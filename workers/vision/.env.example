# Vision Worker Environment Variables

# Database
DATABASE_URL=postgresql://user:password@host:port/database

# Storage Backend (gcs, s3, or supabase)
STORAGE_BACKEND=supabase

# GCS Storage (if using GCS)
GCS_BUCKET_NAME=documentai-storage
GCS_PROJECT_ID=your-project-id
GOOGLE_APPLICATION_CREDENTIALS=/path/to/credentials.json

# S3 Storage (if using S3)
S3_BUCKET_NAME=documentai-storage
S3_ENDPOINT_URL=https://s3.amazonaws.com
S3_ACCESS_KEY_ID=your-access-key
S3_SECRET_ACCESS_KEY=your-secret-key

# Supabase Storage (if using Supabase)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_BUCKET_NAME=documentai-storage

# Vision AI Providers
OPENAI_API_KEY=sk-...
GEMINI_API_KEY=...

# Optional: Override default models
VISION_PROVIDER=openai
VISION_MODEL=gpt-4o-mini

# Logging
LOG_LEVEL=INFO
ENVIRONMENT=production
